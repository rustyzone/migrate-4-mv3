{
  "name": "Simple Scraper",
  "version": "0.01",
  "description": "Simple Scraper",
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Simple Scraper",
    "default_popup": "popout.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3
}
